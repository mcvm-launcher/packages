	"meta": {
		"authors": [],
		"community": "https://discord.gg/mamk7z3TKZ",
		"description": "Integrating Connector with third-party APIs",
		"documentation": "https://sinytra.org/",
		"gallery": [],
		"icon": "https://cdn.modrinth.com/data/FYpiwiBR/e350f02b0298e8c8aa3a7a73b4677636337f0362.png",
		"issues": "https://github.com/Sinytra/ConnectorExtras/issues",
